docker-compose up -d --build

// atualizar a imagem local
docker-compose build .

// subir o container
docker-compose up

//  Entrar no container
docker-compose exec php /bin/bash

// verificar erquisitos do Symfony
symfony check:requirements

// create new Symfony
cd ..
composer create-project symfony/skeleton symfony_docker

//dependencias
composer req --dev maker ormfixtures fakerphp/faker

// create a .env.local file from the existing .env file which Symfony generated during the creation of the project
cp .env .env.local

// Criar entidade
symfony console make:entity User

//remove entidade em:
/src/Entity
/src/Repository

// criar migração
symfony console make:migration


//Adiciona dependencias
composer require annotation
composer require symfony/orm-pack


// listar comando
php bin/console list


// criar tabela na base de dados
php bin/console doctrine:database:create --if-not-exists symfony_docker